{
  "name": "munique-api",
  "version": "1.0.0",
  "description": "API",
  "types": "dist/index.d;ts",
  "main": "dist/index.js",
  "scripts": {
    "dev": "dotenv -e .env.local -- ts-node-dev -r tsconfig-paths/register --respawn --transpile-only index.ts",
    "build": "tsup && tsc-alias",
    "start": "dotenv -e .env.local -- node .",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:generate": "prisma generate"
  },
  "keywords": [],
  "author": "nathan3boss@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@swc/core": "^1.7.26",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "prisma": "^5.20.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsup": "^8.3.0",
    "znv": "^0.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "dotenv-cli": "^7.4.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0"
  }
}
